      <a name="label" val="H"/>
    </comp>
    <comp lib="1" loc="(50,350)" name="D Flip-Flop">
      <a name="label" val="L"/>
    </comp>

    <!-- Stack Pointer -->
    <comp lib="1" loc="200,200" name="D Flip-Flop">
      <a name="label" val="SP"/>
    </comp>

    <!-- Program Counter -->
    <comp lib="1" loc="200,100" name="D Flip-Flop">
